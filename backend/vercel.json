{
  "version": 2,
  "buildCommand": "npm run build",
  "functions": {
    "index.js": {
      "runtime": "@vercel/node@2.15.0"
    }
  },
  "routes": [
    {
      "src": "/users/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/posts/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/comments/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/likes/(.*)",
      "dest": "index.js"
    }
  ]
}
